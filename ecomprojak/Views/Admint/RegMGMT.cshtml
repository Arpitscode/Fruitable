
@{
    ViewBag.Title = "RegMGMT";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
@model List<ecomprojak.Models.Tbl_Register>
<h1 class="text-center text-uppercase">Table of Registers</h1>
<div class="row m-2">
    <table class="table border" style="border:2px solid gray;">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Password</th>
            <th>Mobile Number</th>
            <th>Profile</th>
            <th>Address</th>
            <th>Regdate</th>
            <th></th>
            <th></th>
        </tr>
        @{
            foreach (var x in Model)
            {
                <tr>
                    <td>@x.id</td>
                    <td>@x.name</td>
                    <td>@x.email</td>
                    <td>@x.password</td>
                    <td>@x.mob</td>
                    <td><img src="~/Content/img/@x.profile" width="50" height="50" /></td>
                    <td>@x.address</td>
                    <td>@x.regdate</td>
                    <td><a href="/Admint/Registerdelete?em=@x.email" class="btn btn-outline-danger">Delete</a></td>
                    <td><a href="/Admint/Update?em=@x.email" class="btn btn-outline-success">Update</a></td>
                </tr>
            }
        }
    </table>
</div>

